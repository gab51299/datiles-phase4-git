<div class="pb-2 mt-4 mb-2 border-bottom">
      Page header
</div>

<p id="notice"><%= notice %></p>

<h1>Personal</h1>

<table>
  <thead>
    <tr>
      <th>Email</th>
      <th>Tel</th>
      <th>Mobile</th>
      <th>Id Type</th>
      <th>Reward Type</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @personals.each do |personal| %>
      <tr>
        <td><%= personal.email %></td>
        <td><%= personal.tel %></td>
        <td><%= personal.mobile %></td>
        <td><%= personal.idtype %></td>
        <td><%= personal.rewardtype %></td>
        <td><%= link_to 'Show', personal %></td>
        <td><%= link_to 'Edit', edit_personal_path(personal) %></td>
        <td><%= link_to 'Destroy', personal, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Record', new_personal_path %>
<%= link_to 'Joins', joinp_path(@join) %>
